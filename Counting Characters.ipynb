   "source": [
    "str1=input('Input: ')\n",
    "letter=digit=0\n",
    "for ch in str1:\n",
    "    if ch.isalpha():\n",
    "        letter=letter+1\n",
    "    elif ch.isdigit():\n",
    "        digit=digit+1\n",
    "    else:\n",
    "        continue\n",
    "print('ALPHABETS :', letter)\n",
    "print('DIGITS :', digit)"
   ]
